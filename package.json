{
  "name": "mmir-plugin-encoder-amr",
  "version": "1.0.0",
  "description": "Cordova plugin for the MMIR framework that adds a JavaScript based AMR encoder: uses AMR.js (MIT license), see https://github.com/jpemartins/amr.js",
  "main": "./www/webworker/amrEncoder.js",
  "directories": {
    "lib": "./www"
  },
  "types": "index.d.ts",
  "scripts": {
    "build": "createplugincompat ./ && pluginexport ./"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmig/mmir-plugin-encoder-amr.git"
  },
  "keywords": [
    "audio",
    "codec",
    "amr"
  ],
  "author": "russa",
  "license": "MIT",
  "cordova": {
    "id": "mmir-plugin-encoder-amr",
    "platforms": []
  },
  "engines": {
    "cordovaDependencies": {
      "1.0.0": {
        "https://github.com/mmig/mmir-plugin-encoder-core.git": ">=1.0.0"
      }
    }
  },
  "mmir": {
    "workers": [
      "./www/webworker"
    ],
    "modes": {
      "min": {
        "./www/amrnb.js": "./www/amrnb.min.js"
      }
    }
  },
  "dependencies": {
    "mmir-plugin-encoder-core": "git+https://github.com/mmig/mmir-plugin-encoder-core.git"
  },
  "peerDependencies": {
    "mmir-lib": "git+https://github.com/mmig/mmir-lib.git"
  },
  "devDependencies": {
    "mmir-plugin-exports": "git+https://github.com/mmig/mmir-plugin-exports.git"
  }
}
